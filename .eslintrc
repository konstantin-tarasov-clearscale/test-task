{
  "settings": {
    "react": {
      "version": "17"
    }
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "parser":"@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "react/jsx-indent": ["error", 2],
    "object-curly-spacing": ["error", "always"]
  }
}